{
  "name": "DataRice",
  "version": "0.1.0",
  "private": true,
  "bin": "index.js",
  "pkg": {
    "assets": [
      "./.next/**/*",
      "node_modules/socket.io/"
    ],
    "scripts": [
      "./.next/dist/**/*.js"
    ]
  },
  "scripts": {
    "dev": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "build:web": "next build",
    "build:bin": "pkg . --debug --targets node18-linux-x64 --out-path dist",
    "build:full": "rimraf ./.next/ ./dist/ && npm ci && npm run build:web && npm run build:bin",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.3.0",
    "chokidar": "^3.5.3",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "next": "13.2.3",
    "node-tail": "^0.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "swr": "^2.1.4"
  },
  "devDependencies": {
    "@types/react": "18.2.6",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.7.0",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.1"
  }
}
